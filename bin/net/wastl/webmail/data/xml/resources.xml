<?xml version="1.0" encoding="UTF-8"?>

<!-- This file contains String resources for WebMail that may be extended to support more languages -->

<!DOCTYPE BUNDLE SYSTEM "resource.dtd">

<BUNDLE default="en">
  
  <!-- Section common to all locales -->
  <COMMON>
    <!-- First we have the different html document names -->
    <RESOURCE name="LoginScreen">html-templates/loginscreen.html</RESOURCE>
    <RESOURCE name="send result">html-templates/mailboxscreen_sendresult.html</RESOURCE>
    <RESOURCE name="error">html-templates/mailboxscreen_error.html</RESOURCE>
    <RESOURCE name="folders">html-templates/mailboxscreen_folders.html</RESOURCE>
    <RESOURCE name="adminlogin">html-templates/adminlogin.html</RESOURCE>
    <RESOURCE name="adminform">html-templates/adminform.html</RESOURCE>
    <RESOURCE name="adminuser">html-templates/adminuser.html</RESOURCE>
    <RESOURCE name="adminsessions">html-templates/adminsessions.html</RESOURCE>
    <RESOURCE name="adminvdom">html-templates/adminvdom.html</RESOURCE>

    <RESOURCE name="GNU License">html-templates/COPYING</RESOURCE>
  </COMMON>

  <!-- Language specific parts for english language -->
  <LOCALE lang="en">

    <RESOURCE name="no subject">(no subject)</RESOURCE>
    <RESOURCE name="no date">(no date)</RESOURCE>
    <RESOURCE name="unknown sender">(unknown sender)</RESOURCE>
    <RESOURCE name="unknown recipient">(unknown recipient)</RESOURCE>

    <RESOURCE name="reply subject prefix">RE:</RESOURCE>
    <RESOURCE name="reply subject postfix"></RESOURCE>
    <RESOURCE name="reply message prefix">You wrote:</RESOURCE>
    <RESOURCE name="reply message postfix"></RESOURCE>

    <RESOURCE name="forward subject prefix">[fwd:</RESOURCE>
    <RESOURCE name="forward subject postfix">]</RESOURCE>
    <RESOURCE name="forward message prefix">================== begin forwarded message ==================</RESOURCE>
    <RESOURCE name="forward message postfix">=================== end forwarded message ===================</RESOURCE>

  </LOCALE>

</BUNDLE>
